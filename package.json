{
  "name": "@magicbell/core",
  "version": "4.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "description": "Official MagicBell API wrapper",
  "author": "MagicBell <bot@magicbell.io> (https://magicbell.com/)",
  "contributors": [
    "Bilal Budhani <bilal@bilalbudhani.com>",
    "Josue Montano <josue@magicbell.io>"
  ],
  "main": "dist/index.js",
  "module": "dist/core.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsdx build",
    "prepare": "yarn build",
    "start": "tsdx watch",
    "lint": "tsdx lint --write",
    "report-coverage": "codecov",
    "test": "TZ=America/La_Paz tsdx test --collectCoverage --no-cache",
    "test:watch": "TZ=America/La_Paz tsdx test --watch",
    "console": "ts-node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jsx?,tsx?}": [
      "tsdx lint ./src --write",
      "prettier --write"
    ],
    "*.{md,html,css,scss}": "prettier --write"
  },
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@types/humps": "^2.0.1",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/rosie": "^0.0.40",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-jest": "^27.0.6",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^10.5.3",
    "miragejs": "^0.1.41",
    "prettier": "^2.3.2",
    "rosie": "^2.1.0",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.0",
    "tsdx": "^0.14.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "ably": "^1.2.13",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "dompurify": "^2.3.1",
    "humps": "^2.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "pluralize": "^8.0.0",
    "tslib": "^2.3.1",
    "valtio": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magicbell-io/node-client"
  },
  "keywords": [
    "bell",
    "magicbell",
    "notifications",
    "notification center",
    "notification inbox",
    "reactive",
    "typescript"
  ],
  "homepage": "https://magicbell.com",
  "bugs": {
    "url": "https://github.com/magicbell-io/node-client/issues"
  }
